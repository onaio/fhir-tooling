{
  "request": {
      "method": "PUT",
      "url": "OrganizationAffiliation/$unique_uuid",
      "ifMatch" : "$version"
  },
  "resource": {
    "resourceType": "OrganizationAffiliation",
    "id": "$unique_uuid",
    "identifier": [
      {
        "use": "official",
        "value": "$identifier_uuid"
      }
    ],
    "active": true,
    "organization": {
      "reference": "Organization/$orgID",
      "display": "$orgName"
    },
    "location": []
  }
}
