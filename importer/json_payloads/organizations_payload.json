{
    "request": {
        "method": "PUT",
        "url": "Organization/$unique_uuid",
        "ifMatch" : "$version"
    },
    "resource": {
        "resourceType": "Organization",
        "active": "$active",
        "name": "$name",
        "id": "$unique_uuid",
        "identifier": [
            {
            "value": "$identifier_uuid",
            "use": "official"
            }
        ],
        "alias": [
            "$alias"
        ],
        "type": [
            {
            "coding": [
                {
                "code": "team",
                "system": "http://terminology.hl7.org/CodeSystem/organization-type"
                }
            ]
            }
        ]
    }
}