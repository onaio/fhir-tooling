{
    "request": {
        "method": "PUT",
        "url": "Location/$unique_uuid",
        "ifMatch" : "$version"
    },
    "resource": {
        "resourceType": "Location",
        "id": "$unique_uuid",
        "status": "$status",
        "name": "$name",
        "partOf": {
            "reference": "Location/$parentID",
            "display": "$parentName"
        },
        "physicalType": {
            "coding": [
                {
                    "system": "http://terminology.hl7.org/CodeSystem/location-physical-type",
                    "code": "$pt_code",
                    "display": "$pt_display"
                }
            ]
        }
    }
}